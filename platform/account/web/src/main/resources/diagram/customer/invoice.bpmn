  <process id="customerInvoiceAudit" name="开票资料审核" isExecutable="true">
    <documentation>invoice_data</documentation>
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="auditInvoice" name="财务人员审核" activiti:candidateUsers="${auditUser}"></userTask>
    <userTask id="editInvoice" name="编辑,上传资料"></userTask>
    <sequenceFlow id="flow6" sourceRef="startevent1" targetRef="auditInvoice"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="editInvoice" targetRef="auditInvoice"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" name="审核通过" sourceRef="auditInvoice" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow9" name="审核通未通过" sourceRef="exclusivegateway1" targetRef="editInvoice">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditPass == 0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="审核通过" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${auditPass == 1}]]></conditionExpression>
    </sequenceFlow>
  </process>
