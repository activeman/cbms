#set($layout="layout/blank.vm")
<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
    <title>品牌店代运营系统</title>
    <!-- meta S -->
    #parse("layout/meta.vm")
    <!-- meta E -->
    <!-- meta S -->
    #parse("layout/context.vm")
    <!-- meta E -->
</head>

<body class="no-skin">
<!-- Header S-->
#parse("layout/header.vm")
<!-- Header E -->

<div class="main-container" id="main-container">
    <script type="text/javascript">
        try{ace.settings.check('main-container' , 'fixed')}catch(e){}
    </script>

    <!-- Sidebar S-->
    #parse("layout/sidebar.vm")
    <!-- Sidebar E -->

    <div class="main-content">
        <div class="main-content-inner">
            <div class="breadcrumbs" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        	客户管理
                    </li>
                    <li class="active">我的买家客户</li>
                </ul><!-- /.breadcrumb -->
            </div>
            <!-- content S -->
            <div class="page-content">
						<div class="row">
							<div class="col-xs-12">
								<!-- PAGE CONTENT BEGINS -->
								<div class="row">
									<div class="col-xs-12">
										<div class="clearfix">
                                            <div class="pull-left">
                                                <button id="addAccount" class="btn btn-primary" title="添加">
                                                    <i class="ace-icon glyphicon glyphicon-plus align-top bigger-125"></i>
                                                    添加
                                                </button>
                                            </div>

                                            <div class="input-group pull-right col-md-8 col-sm-9 col-lg-6 text-right">
                                                <label>公司名称：</label><input id="accountName" class="" type="text" value="" />
                                                <label>联系人：</label><input id="contactName" class="" type="text" value="" />  
                                                  <label>联系人手机号码：</label><input id="contactTel" class="" type="text" value="" />                                                                                                 <span class="input-group-btn">
                                                    <button id="searchVendorlist" class="btn btn-sm btn-default" type="button">
                                                        <i class="aace-icon fa fa-search bigger-110"></i>
                                                    </button>
                                            </div>
										</div>

										<!-- div.dataTables_borderWrap -->
										<div>
											<table id="dynamic-table" class="table table-striped table-bordered table-hover">
												<thead><tr><th>公司名称</th><th>经营品种</th><th>代理钢厂</th><th>联系人</th><th>手机</th><th>注册时间</th><th>状态</th><th>操作</th></tr></thead>
											</table>
										</div>
									</div>
								</div>

							</div><!-- /.col -->
						</div><!-- /.row -->
					</div><!-- /.page-content -->
            <!-- content E -->
        </div>
    </div><!-- /.main-content -->

    <!-- Footer S -->
    #parse("layout/footer.vm")
    <!-- Footer E -->

    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
        <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
    </a>
</div><!-- /.main-container -->

<!-- basic scripts S -->
#parse("layout/scripts.vm")
<!-- basic scripts E -->
<input type="hidden" id="account_type" value="$!{type}"/>
<script src="$link.getContextPath()/js/views/account/buyer.js"></script>
</body>
</html>