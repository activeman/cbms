<!-- 待审核-->
<div class="breadcrumbs" id="breadcrumbs">
    <script type="text/javascript">
        try {
            ace.settings.check('breadcrumbs', 'fixed')
        } catch (e) {
        }
    </script>

    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="#">发票管理</a>
        </li>
        <li>
            <a href="#">销项票管理</a>
        </li>
        <li class="active">待录入</li>
    </ul>
    <!-- /.breadcrumb -->

    #parse("layout/topbtn.vm")
    <!-- /.nav-search -->
</div>

<div class="page-content">
    <div class="row">
        <div class="col-xs-12">
            <!--TAPBAR-->
            <!-- tapbar S -->
            #invoiceTopBar(5, $inputInvoiceTotal)
            <!-- tapbar E -->

            <!-- PAGE CONTENT BEGINS -->
            <div class="row">
                <div class="col-xs-12">
                    <div class="clearfix">
                        <div class="input-group pull-right col-md-12 col-sm-12 col-lg-12 search-bar">
                            <form class="form-inline">
                                <label>服务中心：<input placeholder="请选择上级服务中心" id="sorganization" type="text"
                                                   class="ipt-text" readonly="" value="" onclick="showMenu();">
                                    <input type='hidden' id='sorganizationHidden' value='0'/>
                                </label>
                                <label>买家全称：</label><input id="buyerName" search='account' class="ipt-text" type="text"
                                                           value=""/>
                                <label>时间：</label>
                                <input id="startTime" class="Wdate" type="text" value="$startTime"
                                       onFocus="WdatePicker({maxDate:'#F{$dp.$D(\'endTime\')||\'2020-10-01\'}'})"/> -
                                <input id="endTime" class="Wdate" type="text" value="$endTime"
                                       onFocus="WdatePicker({minDate:'#F{$dp.$D(\'startTime\')}',maxDate:'2020-10-01'})"/>

                                <button id="queryBtn" class="btn btn-sm btn-default" type="button" title="搜索">
                                    <i class="aace-icon fa fa-search bigger-110"></i>搜索
                                </button>
                            </form>
                        </div>
                    </div>

                    <!-- div.dataTables_borderWrap -->
                    <div>
                        <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                            <thead>
                            <tr>
                                <th>买家全称</th>
                                <th>开票重量(吨)</th>
                                <th>开票金额(元)</th>
                                <th>时间</th>
                                <th>发票号</th>
                                <th>操作</th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</div>
<div id="menuContent" class="menuContent" style="display:none; position: absolute;">
    <ul id="orgTree" class="ztree" style="margin-top:0; width:160px;"></ul>
</div>
<input type="hidden" id="inputinvoiceinfo" value='$shiro.hasPermission("invoice:out:inputinvoiceinfo:page")' />

<script src="$link.getContextPath()/js/views/invoice/inputinvoicelist.js"></script>


