#if($order)
<div class="main-content-inner">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try {
                ace.settings.check('breadcrumbs', 'fixed')
            } catch (e) {
            }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                  	交易管理
            </li>
            <li>
                <a href="$link.getContextPath()/common/menu/order-view/order-query-index-page.html">交易单管理</a>
            </li>
            #* 面包屑 *#
            #parse("/layout/order/breadcrumbdetail.vm")

            <li class="active">订单详情</li>
        </ul>
        <!-- /.breadcrumb -->

        #parse("layout/topbtn.vm")
        <!-- /.nav-search -->
    </div>
    <div class="page-content">
        <div class="row">
            <div class="col-xs-12">
                <!-- PAGE CONTENT BEGINS -->
                <div class="page-content">
                    <div class="col-xs-12">
                        <div class="step-bar">
                            <div class="ystep4"></div>

                            <div class="ystep1" style="margin-left:200px;"></div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.col -->
            <div class="table-bar">
                <span class="pull-left">
                    <h5 class="h5 bolder">
                        订单信息
                        #if(${menu} == 'applypayment' || ${menu} == 'payment')
                            (<em class="orange">初次付款</em>)
                        #end
                    </h5>
                </span>
                <!-- 增加合同查看页链接(待审核除外) -->
                #if($!{order.status} != 1)
                    <span class="pull-right">
                #if($permission.isChange)
                        <a href="$link.getContextPath()/order/changecontract/print/contract.html?orderChangeId=$!{order.changeOrderId}">查看合同</a>
                #else
                        <a href="$link.getContextPath()/order/contract.html?id=$!{order.id}">查看合同</a>
                #end
                        <a href="javascript:void(0)" id="queryPickup">查看提货单</a>
                    </span>
                #end
                <span id="order_id" class="none">$!{order.id}</span>
                <span id="order_id" class="none">$!{order.id}</span>
                <span id="account_id" class="none">$!{order.accountId}</span>
                <table class="contable">
                    <tbody>
                    <tr>
                        <td class="bg-gray">交易单号：</td>
                        <td>$!{order.code}</td>
                        <td class="bg-gray">来源：</td>
                        <td>网站</td>
                        <td class="bg-gray">开单时间：</td>
                        <td>
                            #if($order && $order.created)
                                ${date.format("yyyy-MM-dd HH:mm:ss",$order.created)}
                            #end
                        </td>
                        <td class="bg-gray">备注：
                            <br><span class="blue" id="note_message">自动保存</span>
                        </td>
                        <td width="200">
                            <textarea class="remarks-textarea" placeholder="可输入备注信息"
                                      id="order_note" modify='#if($permission.audit)true#else false#end'>$!{order.note}</textarea>
                        </td>
                    </tr>
                    <tr>
                        <td class="bg-gray">买家全称：</td>
                        <td>$!{order.accountName}</td>
                        <td class="bg-gray">买家部门：</td>
                        <td>$!{order.departmentName}</td>
                        <td class="bg-gray">买方联系人：</td>
                        <td>$!{order.contactName}</td>
                        <td class="bg-gray">买家联系电话：</td>
                        <td>$!{order.contactTel}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <!--/table-bar  1-->

            <div class="table-bar">
                #set($Integer=0)
                <h5 class="h5 bolder">资源信息</h5>
                <table class="table table-striped table-bordered table-hover orderItemsTable" id="orderItemsTable">
                    <thead>
                    <tr>
                        <th>序号</th>
                        <th>卖家全称</th>
                        <th>部门</th>
                        <th>联系人</th>
                        <th>品名</th>
                        <th>材质</th>
                        <th>规格</th>
                        <th>厂家</th>
                        <th>仓库</th>
                        <th>数量</th>
                        <th>重量<br/>（吨）</th>
                        #if($viewsettleinfo)
                            <th>实提重量<br/>(吨)</th>
                        #end
                        <th>计重<br>方式</th>
                        <th>销售价<br>（元/吨）</th>
                        <th>采购价<br>（元/吨）</th>
                        <th>销售金额<br>（元）</th>
                        <th>采购金额<br>（元）</th>
                        <th class="none">id</th>
                        <th class="none">是否银票支付</th>
                        <th>银票支付</th>
                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody id="itemsdatabody" class="itemsdatabody">
                        #set($totalCostAmount = 0)
                        #if($orderItems)
                            #set ($i=0)
                            #foreach($item in $orderItems)
                                #set($i=$i+1)
                            <tr itemid="$!{item.id}">
                                <td>$i</td>
                                <td>
                                    #if(${order.consignType} == 'temp')
                                        <span><em class='label-orange white'>临</em>$item.sellerName</span>
                                    #else
                                        $item.sellerName
                                    #end
                                </td>
                                <td>$!item.departmentName</td>
                                <td>$!item.contactName</td>
                                <td>$!item.nsortName</td>
                                <td>$!item.material</td>
                                <td>$!item.spec</td>
                                <td>$!item.factory</td>
                                <td>$!item.warehouse</td>
                                <td>$!item.quantity</td>
                                <td class="text-left">$NumberTool.format("#0.000000",$!{item.weight})</td>
                                #if($!viewsettleinfo)
                                    <td class="text-left">$NumberTool.format("#0.000000",$!{item.actualPickWeightServer})</td>
                                #end
                                <td>$!item.weightConcept</td>
                                <td class="text-right">$NumberTool.format("#0.00",$!{item.dealPrice})
                                    #if($!{item.isPayedByAcceptDraft})
                                        <br /><em class="label-orange" draftrelated="${item.isPayedByAcceptDraft}" draftid="$!{item.acceptDraftId}">银票支付</em>
                                    #end
                                </td>
                                <td class="text-right">$NumberTool.format("#0.00",$!{item.costPrice})</td>
                                <td dt="money" class="text-right">$NumberTool.format("#0.00",$!{item.amount})</td>
                                #set($costAmount = $!item.getCostAmount())
                                #set($totalCostAmount = $totalCostAmount + $costAmount)
                                <td dt="money" class="text-right">$NumberTool.format("#0.00", $costAmount)</td>
                                <td class="none orderItemsId">$!{item.id}</td>
                                <td class="none isPayedByAcceptDraft">$!{item.isPayedByAcceptDraft}</td>
                                #if(${item.isPayedByAcceptDraft})
                                    #if($Integer.parseInt(${order.status}) != 2)
                                        <td>
                                            #if($!{item.acceptDraftCode} == "")
                                                待选择票号
                                            #else
                                                $!{item.acceptDraftCode}
                                            #end
                                        </td>
                                    #else
                                        <td>
                                            #if($!{item.acceptDraftCode} == "")
                                                <select name="acceptDraft">
                                                </select>
                                            #else
                                                $!{item.acceptDraftCode}
                                            #end
                                        </td>
                                    #end
                                #else
                                    #if($Integer.parseInt(${order.status}) != 2)
                                        <td>
                                            #if($!{item.acceptDraftCode} == "")
                                                无
                                            #else
                                                $!{item.acceptDraftCode}
                                            #end
                                        </td>
                                    #else
                                        <td>无</td>
                                    #end
                                #end
                                <td>  <!-- 二结后增加查看进项票，二结前增加订单未二结，内容不能点击 -->
                                    #if($!{isSecondCount} == "1")
                                        <a href="javascript:void(0)" class="queryInInvoiceView">查看进项票</a>
                                    #else
                                        订单未二结
                                    #end
                                </td>
                            </tr>
                            #end
                        #end
                    </tbody>
                    <tbody class="tbody-border0">
                    <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right">
                            #if($orderItems&&$orderItems.size()>0)
                                总计:
                            #else
                                暂无卖家信息
                            #end
                        </td>
                        <td>$!{totalItem.quantity}</td>
                        <td class="text-left">$!NumberTool.format("#0.000000",$!{totalItem.weight})</td>
                        #if($viewsettleinfo)
                            <td>$!NumberTool.format("#0.000000",$!{totalItem.actualPickWeightServer})</td>
                        #end
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right"><em dt="money">$NumberTool.format("#0.00",$!{totalItem.amount})</em><input type="hidden" id="order_total_amount" value="$!{totalItem.amount}"></td>
                        <td class="text-right"><em dt="money">$NumberTool.format("#0.00",$totalCostAmount)</em></td>
                        <td></td>
                        <td></td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <div id="invoiceView" style="display: none"></div>
            <!--/table-bar 2-->
            <div class=" recordbar col-xs-12 text-left">
                <span class="bolder">交（提）货信息：</span>
                <span>交货方式：<span class="bolder">$!{order.deliveryType}</span></span>
                <span>交货地址：<span class="bolder">#if($!{order.transArea}) $!{order.transArea} #else 货物所在仓库 #end</span></span>
                <span>交货时间：<span class="bolder">
                    #if($order.deliveryEndDate)
                       $date.format("yyyy-MM-dd",$!{order.deliveryEndDate})
                    #end
                </span></span>
                <span>运费：由
                    <em class="bolder">&nbsp;$!{order.feeTaker}&nbsp;</em>承担
                    <span class="bolder">$NumberTool.format("#0.00",$!{order.shipFee})元</span>
                </span>
                <span>出库费：由
                    <em class="bolder">&nbsp;$!{order.outboundTaker}&nbsp;</em>承担
                    <span class="bolder">$NumberTool.format("#0.00",$!{order.outboundFee})元</span>
                </span>
            </div>

        </div>
        <!-- /.row -->
        #if($order.status==2)
            <!--settlement-info s-->
            <div class="settlement-info bg-yellow-style row">
                <div class="clearfix">
                    <span>资金账户余额（元）：<em class="red bolder" id="account_balance">$!NumberTool.format(
                        "#0.00",$!{account.balance})</em></span>
								<span>
								  <label>
                                      <input name="settleSec" type="checkbox" class="ace"
                                             id="cb_blance_second_settlement">
                                      <span class="lbl">二次结算余额（元）：</span><em class="red bolder"
                                                                             id="account_blance_second_settlement">$!NumberTool.format(
                                      "#0.00",$!{account.balanceSecondSettlement})</em>#if($!{account.balanceSecondSettlement} > 0)
                                      (可抵扣货款)#end
                                  </label>
								</span>
                                <span>
								  <label>
                                      <input name="creditUsed" type="checkbox" class="ace"
                                             id="cb_blance_credit" checked>
                                      <span class="lbl">可用信用额度（元）：</span><em class="red bolder"
                                                                             id="account_blance_credit">$!NumberTool.format(
                                      "#0.00",$!{balanceCreditAmount})</em>#if($!{balanceCreditAmount} > 0)
                                      (可抵扣货款)#end
                                  </label>
								</span>
                    <span>可用余额（元）：<em class="red bolder" id="account_total_balance">0.00</em></span>
                </div>
            </div>

            <!-- 超出允许的容差范围，无法关联的提示-->
            <div class="settlement-info bg-yellow-style row none" id="insufficient_fund">
                <div class="clearfix">
                <span class="bolder red">可用金额少于订单金额<em
                        id="insufficient_fund_balance">0.00</em>元，超出允许的容差范围，无法关联，请先充值</span>
                </div>
            </div>

            <!-- 在差额允许范围内 可关联的提示-->
            <div class="settlement-info bg-green-style row none" id="allow_releted_info">
                <div class="clearfix">
                <span class="bolder green">可用金额少于订单金额<em
                        id="sub_balance">0.00</em>元，在允许的容差范围内，可以关联
                </span>
                </div>
            </div>


            <!--买家金额容差值 -->
            <input type="hidden" value="$!buyerToleranceAmount" id="buyerToleranceAmount"/>
            <!-- 合同关联金额 -->
            <input type="hidden" value="" id="contact_releted_amount"/>

            <!--settlement-info e-->
        #end

        <div class="row">
            #if($permission.applypay&&$sellerInfoList)
                <div class="table-bar">
                    <h5 class="h5 bolder">付款信息</h5>
                    <table class="table table-striped table-bordered table-hover" id="payment_table">
                        <thead>
                        <tr>
                            <th>付款单位</th>
                            <th>付款单位开户行</th>
                            <th>付款银行账号</th>
                            <th class="none">联系电话</th>
                            <th class="none">注册地址</th>
                            <th>合同付款金额</th>
                            <th>抵扣二次结算<br>账户欠款</th>
                            <th>还信用额度</th>
                            <th>申请付款金额</th>
                            <th class="none">状态</th>
                            <th>操作</th>
                        </tr>
                        </thead>
                        <tbody>
                            #foreach($item in $sellerInfoList)
                            <tr accountid="$!{item.sellerId}" departmentId="$!{item.sellerDepartmentId}" contractid="$!{item.contractId}">
                                <td>$!{item.companyName}$!{item.sellerDepartmentName}</td>
                                <td><select class="" style="width:220px;">
                                    <option value=''>未找到银行账号信息</option>
                                </select></td>
                                <td><span></span></td>
                                <td class="none">$!{item.tel}</td>
                                <td class="none">$!{item.regAddress}</td>
                                <td dt="money" class="text-right">$NumberTool.format("#0.00",$!{item.contractAmount})</td>
                                <td class="text-right">
                                    #if($item.balanceSecondSettlement < 0)
                                        #if($permission.applypay)
                                            <input type="checkbox" name="secondSettlementCheck" cbsecondsettlement="$!{item.balanceSecondSettlement}" />
                                        #end
                                        <span dt="money">$!{NumberTool.toThousandth($!{item.balanceSecondSettlement})}</span>
                                    #else
                                        $!{NumberTool.toThousandth(0)}
                                    #end
                                </td>
                                <td class="text-right">
                                    #if($item.creditUsed > 0)
                                        <input type="checkbox" name="refundcredit" cbrefundcredit="$!{item.creditUsed}"/>
                                        <span dt="money">$!{NumberTool.toThousandth($!{item.creditUsed})}</span>
                                    #else
                                        $!{NumberTool.toThousandth(0)}
                                    #end
                                </td>
                                <td>
                                    <input type="text" verify="rmb" name="paymentAmount" contractAmount="$!{item.contractAmount}"/></td>
                                <td class="none">未申请付款<br/>
                                    #if($!{item.contractStatus}==0)
                                        待上传合同
                                    #elseif($!{item.contractStatus}==1)
                                        合同已上传
                                    #end
                                </td>
                                <td class="text-center">
                                    #if($permission.applypay)
                                        #if($!{item.contractStatus}==0)
                                            <a href="javascript:void(0)" class="applypay_contract_upload  btn-info btn-sm">上传合同</a>
                                        #elseif($!{item.contractStatus}==1)
                                            <a href="javascript:void(0)" class="applypay_contract_view  btn-info btn-sm" >查看合同</a>&nbsp;
                                            <a href="javascript:void(0)" class="applypay_contract_upload  btn-info btn-sm" > 重新上传 </a>
                                        #end
                                    #end
                                </td>
                            </tr>
                            #end
                        </tbody>
                    </table>
                </div>
            #elseif($payRequestList)
                <div class="table-bar">
                    <h5 class="h5 bolder">付款信息</h5><span id="payrequest_id" class="none">$!{requestId}</span>
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>付款单位</th>
                            <th>付款单位开户行</th>
                            <th>付款银行账号</th>
                            <th>联系电话</th>
                            <th>注册地址</th>
                            <th>付款金额</th>
                            <th>状态</th>
                            <td>操作</td>
                        </tr>
                        </thead>
                        <tbody>
                            #foreach($item in $payRequestList)
                            <tr contractid="$!{item.contractId}" requestitemid="$!{item.id}">
                                <td>$!{item.receiverName}$!{item.sellerDepartmentName}</td>
                                <td>$!{item.receiverBankName}$!{item.receiverBranchBankName}</td>
                                <td>$!{item.receiverAccountCode}</td>
                                <td>$!{item.receiverTel}</td>
                                <td>$!{item.receiverRegAddr}</td>
                                <td dt="money" class="text-right">$NumberTool.format("#0.00",$!{item.payAmount})</td>
                                <td>
                                    #if($order.payStatus=='REQUESTED' )
                                        已申请待审核
                                    #elseif($order.payStatus=='APPROVED')
                                        已通过付款申请
                                    #elseif($order.payStatus=='PAYED')
                                        已付款
                                    #end
                                </td>
                                <td>
                                	<a href="javascript:void(0)" class="applypay_contract_view">查看合同</a><br>
                                	#if($order.payStatus=='PAYED')
                                		<a href="javascript:void(0)" class="receipt_view">查看回执单</a>
									#end
									#if($!{item.type} == "5")
										<a href="$link.getContextPath()/account/payment/paymentrequest.html?id=$!{item.changeOrderId}&requestId=$!{item.requestId}" >查看付款申请</a>
									#else
										<a href="$link.getContextPath()/account/payment/paymentrequest.html?id=$!{order.id}" >查看付款申请</a>
									#end
                                </td>
                            </tr>
                            #end
                        </tbody>
                        <tbody class="tbody-border0">
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>合计</td>
							#set($totalAmount = 0)
							#foreach($item in $payRequest)
								#set($totalAmount = $totalAmount + $item.totalAmount)
							#end
							<td dt="money" class="text-right">$NumberTool.format("#0.00",$totalAmount)</td>
							<td colspan="2"></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            #end
            #if($viewsettleinfo)
                <div class="table-bar">
                    <h5 class="h5 bolder">二次结算</h5>
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>买家单位全称</th>
                            <th>买家开户行</th>
                            <th>买家银行账号</th>
                            <th>买家联系电话</th>
                            <th>买家注册地址</th>
                            <th>已收金额</th>
                            <th>结算金额</th>
                            <th>应收金额</th>
                            <th>应付金额</th>
                            <th>账户余额</th>
                        </tr>
                        </thead>
                        <tbody>
                            #foreach($item in $buyersettle)
                            <tr>
                                <td>$!item.companyName</td>
                                <td>$!item.account_BankName</td>
                                <td>$!item.account_BankCode</td>
                                <td>$!item.account_Tel</td>
                                <td>$!item.account_Address</td>
                                <td class="text-right">$!item.amount</td>
                                <td class="text-right">$!item.settleAmount</td>
                                <td class="text-right">$!NumberTool.format("#0.00",$item.receiveAmount)</td>
                                <td class="text-right">$!NumberTool.format("#0.00",$item.payAmount)</td>
                                <td class="text-right">$!NumberTool.format("#0.00",$item.balance)</td>
                            </tr>
                            #end
                        </tbody>
                    </table>

                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>卖家单位全称</th>
                            <th>卖家开户行</th>
                            <th>卖家银行账号</th>
                            <th>卖家联系电话</th>
                            <th>卖家注册地址</th>
                            <th>已付金额</th>
                            <th>结算金额</th>
                            <th>应收金额</th>
                            <th>应付金额</th>
                            <th>账户余额</th>
                        </tr>
                        </thead>
                        <tbody>
                            #set($totalAmount = 0)
                            #set($totalSettleAmount = 0)
                            #set($totalReceiveAmount = 0)
                            #set($totalPayAmount = 0)
                            #foreach($item in $sellersettle)
                            <tr>
                                #set($totalAmount = $totalAmount + $item.amount)
                                #set($totalSettleAmount = $totalSettleAmount + $item.settleAmount)
                                #set($totalReceiveAmount = $totalReceiveAmount + $item.receiveAmount)
                                #set($totalPayAmount = $totalPayAmount + $item.payAmount)
                                <td>$!item.companyName</td>
                                <td>$!item.account_BankName</td>
                                <td>$!item.account_BankCode</td>
                                <td>$!item.account_Tel</td>
                                <td>$!item.account_Address</td>
                                <td class="text-right">$!item.amount</td>
                                <td class="text-right">$!item.settleAmount</td>
                                <td class="text-right">$NumberTool.format("#0.00",$!item.receiveAmount)</td>
                                <td class="text-right">$NumberTool.format("#0.00",$!item.payAmount)</td>
                                <td class="text-right">$NumberTool.format("#0.00",$!item.balance)</td>
                            </tr>
                            #end
                        </tbody>
                        <tbody class="tbody-border0">
                        <tr>
                            <td>
                                #if($sellersettle&&$sellersettle.size()>0)
                                    合计:
                                #else
                                    暂无卖家信息
                                #end
                            </td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td class="text-right">$totalAmount</td>
                            <td class="text-right">$totalSettleAmount</td>
                            <td class="text-right">$NumberTool.format("#0.00",$totalReceiveAmount)</td>
                            <td class="text-right">$NumberTool.format("#0.00",$totalPayAmount)</td>
                            <td></td>
                        </tr>
                        </tbody>
                    </table>
